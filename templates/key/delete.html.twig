{% extends "index.html.twig" %}
{% block title %}Delete{% endblock title %}
{% block content %}
<header>
    <h1>Confirm Delete Key</h1>

</header>
<main>


    {{ form_start(formdata) }}
    <div>
        {{ form_errors(formdata) }}
    </div>

    <div>
        <div>
           <h4>Do you want to delete the Key {{ formdata.text_key.vars.value }} and its messages?</h4>
        </div>

        <h4>Messages</h4>
        {% for message in formdata.translationmessages %}
            <p>
                <strong>{{message.language.vars.value}}</strong>
                {{message.message.vars.value}}</p>

        {% endfor %}


        <div>
            {{ form_row(formdata.submit) }}
        </div>

    </div>
    {{ form_end(formdata) }}
    <ul>

        <li><a class="a_link"href="{{path("index_keys")}}">Back</a></li>
    </ul>
</main>
{% endblock content %}